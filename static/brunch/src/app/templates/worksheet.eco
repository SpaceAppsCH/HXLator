<div id="worksheet-view">
  Please select the headers and data in this worksheet. Highlight all headers first (blue color) and
then please highlight the data (grey color)
  <table>
    <% if @rows: %>
    <% for row in @rows: %>
    <tr>
      <% for cell,index in row: %>
      <td id="<%= @rows.indexOf(row) + 1 %>-<%= index + 1 %>"  data-value="<%= cell %>"><%= cell %></td>
      <% end %>
    </tr>
    <% end %>
  </table>
</div>

